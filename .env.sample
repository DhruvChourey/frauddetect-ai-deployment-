# Copy this file to .env and fill in production values
# PROVIDER choices: mock | gemini | openai
PROVIDER=mock

# Gemini (Google proprietary AI) API key and optional model
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.0-flash

# OpenAI API key and optional model
OPENAI_API_KEY=
OPENAI_MODEL=gpt-3.5-turbo

# Legacy mock flag (PROVIDER preferred)
USE_MOCK=true

PORT=3000

